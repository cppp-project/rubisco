#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:90
msgid "Source directory"
msgstr "源代码目录"

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:91
msgid "Source directory."
msgstr "源代码目录."

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:98
#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:102
msgid "Make source package."
msgstr "创建源代码包."

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:106
msgid "Destination"
msgstr "目标"

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:109
msgid "Destination directory."
msgstr "目标目录."

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:114
msgid "Archive type"
msgstr "归档类型"

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:118
msgid "Archive type of the source distribution. Use '+' to make different archives. e.g. 'zip+tar.gz'. Use 'all' to select all supported types. Use 'none' to generate a directory instead of a archive. Defaults to 'all'."
msgstr "源代码分发的归档类型。使用 '+' 来创建不同的归档。例如 'zip+tar.gz'。使用 'all' 选择所有支持的类型。使用 'none' 生成一个目录而不是归档。默认为 'all'。"

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:128
msgid "Keep source directory"
msgstr "保留源代码目录"

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:132
msgid "Keep source directory. If `archive-type` is 'none', this option will be enabled. Defaults to 'false'."
msgstr "保留源代码目录。如果 `archive-type` 是 'none'，则此选项将被启用。默认为 'false'。"

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:139
msgid "Name format"
msgstr "名称格式"

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/events.py:143
msgid "package name format. Defaults to '<project-name>-<version>'"
msgstr "软件包名称格式。默认为 '<project-name>-<version>'"

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/dist.py:101
msgid "Making source package for project: [cyan]${{name}}[/cyan] ..."
msgstr "正在为项目创建源代码包: [cyan]${{name}}[/cyan] ..."

#: /workspaces/cppp/rubisco/extensions/cppp-srcpkg/cppp_srcpkg/preprocessor.py:48
msgid "Missing include file path in file ${{file}}."
msgstr "缺失文件 ${{file}} 中的包含文件路径."
